    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-dts": "^1.6.5",
