    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "bumpp": "^8.2.1",
    "codesandbox-import-util-types": "~2.2.3",
    "concurrently": "^7.4.0",
    "core-js": "^3.25.5",
    "cssnano": "^5.1.13",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
