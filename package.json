{
  "name": "sandpack-vue3",
  "version": "3.0.1",
  "description": "Vue3 components that give you the power of editable sandboxes that run in the browser.",
  "keywords": [
    "vue3",
    "sandpack",
    "sandpack-vue",
    "sandpack-vue3",
    "@codesandbox",
    "@codesandbox/sandpack-vue",
    "@codesandbox/sandpack-vue3",
    "codesandbox"
  ],
  "homepage": "https://sandpack-vue3.netlify.app",
  "bugs": {
    "url": "https://github.com/jerrywu001/sandpack-vue3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrywu001/sandpack-vue3.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "global.d.ts"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup && npm run build:css",
    "build:css": "postcss src/styles/index.css -o dist/index.css",
    "build:css-watch": "postcss src/styles/index.css -o dist/index.css -w",
    "build:storybook": "npm run build:storybook -w playground",
    "build:vite": "vite build && npm run build:css",
    "clean": "rimraf -rf ./node_modules && rimraf -rf playground/node_modules && rimraf -rf package-lock.json",
    "dev": "concurrently \"npm run build:css-watch\" \"npm run dev -w playground\"",
    "dev:pack": "npm run dev:usepack -w playground",
    "lint": "eslint --ext .ts,.tsx ./src",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "storybook": "npm run storybook -w playground",
    "stylelint": "stylelint **/*.{css,less,scss,vue}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc && vue-tsc"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.4.2",
    "@codemirror/commands": "^6.2.2",
    "@codemirror/lang-css": "^6.1.1",
    "@codemirror/lang-html": "^6.4.2",
    "@codemirror/lang-javascript": "^6.1.4",
    "@codemirror/language": "^6.6.0",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.9.2",
    "@codesandbox/sandpack-client": "^2.1.0",
    "@lezer/highlight": "^1.1.3",
    "@stitches/core": "^1.2.8",
    "ansi-to-vue3": "^0.1.1",
    "clean-set": "^1.1.2",
    "code-hike-classer-vue3": "^0.1.6",
    "dequal": "^2.0.3",
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@codemirror/lang-python": "^6.1.2",
    "@codemirror/legacy-modes": "^6.3.1",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@swc/core": "^1.3.40",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^18.15.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/ui": "^0.29.2",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "bumpp": "^9.0.0",
    "concurrently": "^7.6.0",
    "core-js": "^3.29.1",
    "cssnano": "^5.1.15",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "npm": "^9.6.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-nested": "^6.0.1",
    "rimraf": "^4.4.0",
    "shelljs": "^0.8.5",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^3.0.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-windicss": "^1.8.10",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  }
}
